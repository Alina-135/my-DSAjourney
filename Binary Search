#include <stdio.h>
int main(){
    int n, i, target,count=0;
    printf("Enter the number of elements: ");
    scanf("%d",&n);
    
    int arr[n];
    printf("Enter the elements: ");
    for(i=0; i<n; i++){
        scanf("%d",&arr[i]);
    }
    
    printf("Enter the elements you want to find: ");
    scanf("%d",&target);
    
    int low=0, high=n-1, mid;
    int found = 0;
    
    while(low<=high){
        mid=((low+high)/2);
            count++;
        if(arr[mid]==target){
            printf("The elements is found at %d position. \n",mid);
            found=1;
            break;
        }
        else if(arr[mid]<target){
            low = mid + 1;
        }
        else{
            high = mid - 1;
        }
    }
    if(count>0){
        printf("The number of times the %d elements occurs is %d", target,count);
    }
    else{
        printf("The element is not present.");
    }
    return 0;
}
